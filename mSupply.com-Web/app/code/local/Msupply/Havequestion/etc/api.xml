<config>
    <api>
        <resources>
            <havequestion_havequestion translate="title" module="msupply_havequestion">
                <title>Havequestion API</title>
                <model>msupply_havequestion/havequestion_api</model>
                <acl>havequestion/havequestion</acl>
                <methods>
                    <list translate="title" module="msupply_havequestion">
                        <title>Retrieve list of havequestions</title>
                        <method>items</method>
                        <acl>havequestion/havequestion/list</acl>
                    </list>
                    <info translate="title" module="msupply_havequestion">
                        <title>Retrieve havequestion info</title>
                        <acl>havequestion/havequestion/info</acl>
                    </info>
                    <add translate="title" module="msupply_havequestion">
                        <title>Add havequestion</title>
                        <acl>havequestion/havequestion/add</acl>
                    </add>
                    <update translate="title" module="msupply_havequestion">
                        <title>Update havequestion</title>
                        <acl>havequestion/havequestion/update</acl>
                    </update>
                    <remove translate="title" module="msupply_havequestion">
                        <title>Remove havequestion</title>
                        <acl>havequestion/havequestion/remove</acl>
                    </remove>
                </methods>
                <faults module="msupply_havequestion">
                    <havequestion_not_exists>
                        <code>101</code>
                        <message>Requested havequestion does not exist.</message>
                    </havequestion_not_exists>
                    <invalid_data>
                        <code>102</code>
                        <message>Provided data is invalid.</message>
                    </invalid_data>
                    <save_error>
                        <code>103</code>
                        <message>Error while saving havequestion. Details in error message.</message>
                    </save_error>
                    <remove_error>
                        <code>104</code>
                        <message>Error while removing havequestion. Details in error message.</message>
                    </remove_error>
                </faults>
            </havequestion_havequestion>
        </resources>
        <resources_alias>
            <havequestion>havequestion_havequestion</havequestion>
        </resources_alias>
        <v2>
            <resources_function_prefix>
                <havequestion>havequestionHavequestion</havequestion>
            </resources_function_prefix>
        </v2>
        <acl>
            <resources>
                <msupply_havequestion translate="title" module="msupply_havequestion">
                    <title>Havequestion</title>
                    <havequestion translate="title" module="msupply_havequestion">
                        <title>Havequestion</title>
                        <sort_order>0</sort_order>
                        <list translate="title" module="msupply_havequestion">
                            <title>List</title>
                        </list>
                        <info translate="title" module="msupply_havequestion">
                            <title>Info</title>
                        </info>
                        <add translate="title" module="msupply_havequestion">
                            <title>Add</title>
                        </add>
                        <update translate="title" module="msupply_havequestion">
                            <title>Update</title>
                        </update>
                        <remove translate="title" module="msupply_havequestion">
                            <title>Remove</title>
                        </remove>
                    </havequestion>
                </msupply_havequestion>
            </resources>
        </acl>
    </api>
</config>
